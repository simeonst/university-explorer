{
    "title": "COLUMNS keys must be GROUP or APPLY when TRANSFORMATIONS is present",
    "query": {
        "WHERE": {
            "GT": {
                "courses_avg": 70
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_title",
                "courses_avg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "courses_avg"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_title"
            ],
            "APPLY": [
                {
                    "overallAvg": {
                        "MAX": "courses_avg"
                    }
                }
            ]
        }
    },
    "isQueryValid": false,
    "result": "InsightError"
}
